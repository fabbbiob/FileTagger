@model IEnumerable<FileTaggerMVC.Models.FileViewModel>

<script type="text/javascript" src="~/Scripts/fileRunner.js"></script>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FileName)
        </th>
        <th>
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.FileName
            </td>
            <td>
                @{
                    Html.RenderPartial("RunButton", item.FilePath);
                }
            </td>
        </tr>
    }
</table>

